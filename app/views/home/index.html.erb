<%= form_with url: "/rankings", method: :get do |form| %>
<div class="form-group">
    <%= form.label :character, "Character"%>
    <%= form.text_field :character, class: "form-control" %>
</div>
<div class="form-group">
    <%= form.label :server, "Server"%>
    <%= form.select(:server, ['Sulfuron'], {}, { :class => 'form-control' }) %>
</div>
<div class="form-group">
    <%= form.label :region, "Region"%>
    <%= form.select(:region, ['EU'], {}, { :class => 'form-control' }) %>
</div>
<%= form.submit "Submit", class: "btn btn-primary" %>
<% end %>